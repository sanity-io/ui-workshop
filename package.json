{
  "type": "module",
  "name": "@sanity/ui-workshop",
  "version": "1.2.0",
  "sideEffects": false,
  "bin": {
    "workshop": "./bin/workshop.cjs"
  },
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "types": "./dist/index.d.ts",
  "source": "./exports/index.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./exports/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "node": {
        "import": "./dist/node/index.cjs.js",
        "require": "./dist/index.cjs"
      },
      "default": "./dist/index.js"
    },
    "./plugin-a11y": {
      "types": "./dist/plugin-a11y.d.ts",
      "source": "./exports/plugin-a11y.ts",
      "import": "./dist/plugin-a11y.js",
      "require": "./dist/plugin-a11y.cjs",
      "node": {
        "import": "./dist/node/plugin-a11y.cjs.js",
        "require": "./dist/plugin-a11y.cjs"
      },
      "default": "./dist/plugin-a11y.js"
    },
    "./plugin-perf": {
      "types": "./dist/plugin-perf.d.ts",
      "source": "./exports/plugin-perf.ts",
      "import": "./dist/plugin-perf.js",
      "require": "./dist/plugin-perf.cjs",
      "node": {
        "import": "./dist/node/plugin-perf.cjs.js",
        "require": "./dist/plugin-perf.cjs"
      },
      "default": "./dist/plugin-perf.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "source": "./exports/runtime.ts",
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.cjs",
      "node": {
        "import": "./dist/node/runtime.cjs.js",
        "require": "./dist/runtime.cjs"
      },
      "default": "./dist/runtime.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "plugin-a11y": [
        "./dist/plugin-a11y.d.ts"
      ],
      "plugin-perf": [
        "./dist/plugin-perf.d.ts"
      ],
      "runtime": [
        "./dist/runtime.d.ts"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "run-s clean build:pkg check:pkg",
    "build:pkg": "pkg build",
    "check:pkg": "pkg --strict",
    "check:types": "tsc --build",
    "clean": "rimraf dist",
    "commit": "cz",
    "dev": "run-p workshop:dev",
    "format": "prettier --write --cache --ignore-unknown .",
    "lint": "eslint . --ext .cjs,.js,.jsx,.mjs,.ts,.tsx --quiet",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build",
    "release": "semantic-release",
    "test": "jest",
    "watch": "pkg watch --strict",
    "workshop:build": "rimraf .workshop && node --loader esbuild-register/loader -r dotenv-flow/config -r esbuild-register -r tsconfig-paths/register scripts/build",
    "workshop:dev": "node --loader esbuild-register/loader -r dotenv-flow/config -r esbuild-register -r tsconfig-paths/register scripts/dev",
    "workshop:start": "serve -p 1337 -s .workshop/dist"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^3.0.0",
    "axe-core": "^4.6.1",
    "cac": "^6.7.14",
    "chokidar": "^3.5.3",
    "dotenv-flow": "^3.2.0",
    "esbuild": "^0.16.13",
    "esbuild-register": "^3.4.2",
    "express": "^4.18.2",
    "globby": "^11.1.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "pako": "^2.1.0",
    "segmented-property": "^3.0.3",
    "vite": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/config-conventional": "^17.4.2",
    "@sanity/icons": "^2.2.2",
    "@sanity/pkg-utils": "^2.2.3",
    "@sanity/semantic-release-preset": "^4.0.0",
    "@sanity/ui": "^1.2.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.11.18",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-jest": "^29.4.1",
    "commitizen": "^4.3.0",
    "commitlint": "^17.4.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^4.1.2",
    "semantic-release": "^20.1.0",
    "serve": "^14.2.0",
    "styled-components": "^5.3.6",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@sanity/icons": "^2",
    "@sanity/ui": "^1",
    "react": "^18",
    "react-dom": "^18",
    "styled-components": "^5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/ui-workshop.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/ui-workshop/issues"
  },
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/",
  "keywords": [
    "sanity",
    "ui",
    "design",
    "design system",
    "prototyping",
    "testing",
    "qa"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "packageManager": "pnpm@7.26.3",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "extends": "@sanity/semantic-release-preset",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
